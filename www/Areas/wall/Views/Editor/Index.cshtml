@using Resources
@{
    Layout = "../Shared/_Layout.cshtml";
    var _dbid = Request.QueryString["db"];
    var currentLang = ViewBag.CurrentLang;
}

<script type="text/javascript" src="~/areas/extsrv/js/main.js"></script>

<div style="cursor: pointer;width: 80px;position: absolute;top:25px;left: 10px;z-index: 9;" id="Edge"><img src="/Images/m.png" alt="" title="@Client.productsList" /></div>

<img src="~/Images/bigrotation2.gif" alt="" id="Rotor" class="Rotor" />

<div id="MasterCreate"></div>
<div id="ImageGallery"></div>
<div class="ForKit"></div>

<div class="ModeInfo ui-corner-all"><div class="ui-state-highlight" style="margin-bottom: 4px;padding: 2px;"><span class="ui-icon ico" title="@Client.ModeView"></span></div><button class='switch'>&nbsp;</button></div>

<div class="Vitrins ui-corner-top"><div class="ui-corner-top bolt"></div>
    <div class="ListVitrins"></div>    

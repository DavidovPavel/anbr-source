    <div class="Addw ui-state-highlight"><span class="ui-icon ui-icon-circle-plus" title="@Client.addVitrin"></span></div>
</div>

<div id="DemoMode" data-id="FE195E4A-CAAD-4987-A1E8-2788F1796F95" class="ui-corner-right"></div><div id='Terms'></div>
@if(ViewBag.isSupervizer)
{
    <div id="OnlineEditor"></div>
}
<script type="text/javascript">
    //$(document).tooltip({ track: true });
    require.config({
        config: { i18n: { locale: '@currentLang.ToLower()' } }
    });
    define("bdid", "@_dbid");
    define("access",{data: @Html.Raw(ANBR.Helpful.Misc.Js.Helper.JSObjectVariable((object)ViewBag.GLOBAL_SA))});
</script>
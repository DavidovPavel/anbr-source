<div class="filter-title">
    <span class="btn btn-link-clear"><%= R.cancelFilter %></span>
</div>

<div id="selections"></div>

<div class="filter-list">

    <span class="btn-collapse"><%= R.category %></span>
    <div class="collapse">
        <div id="ByPayment"></div>
        <div id="ByStatus"></div>
    </div>

    <span class="btn-collapse"><%= R.selectCountry %></span>
    <div class="collapse in on">
        <div id="ByCountry"></div>
    </div>

    <span class="btn-collapse"><%= R.byTypeSurce %></span>
    <div class="collapse">
        <div id="BySrcKind"></div>
    </div>

    <span class="btn-collapse"><%= R.byInfo %></span>
    <div class="collapse">
        <div id="ByInfo"></div>
    </div>

</div>
@model ABS.WReportsProcessing.Contracts.ModelReportABSGeneral
@{
    Layout = null;// "~/Views/Shared/WReports/_WReportBaseLayout.cshtml";
    int mid = Model.MainObjectID;

    string cssAll = File.ReadAllText(Server.MapPath(@"~/frontend/public/css/dossier.css"));
}

<style type="text/css" media="all">
    @Html.Raw(cssAll)
</style>

<style media="print">
      body {
      padding: 0!important;
    }
    body * {
      position: static!important;
      height: auto!important;
    }
    body > header {
      display: none;
    }
    body > aside[class*='g-sidebar'] {
      display: none;
    }
    body .workbench {
      height: auto;
      position: static;
    }
    body .workbench .workbench--content {
      height: auto;
      position: static;
    }
    body .workbench > .crumbs {
      display: none;
    }
    body .workbench > .g-toolbar {
      display: none;
    }
    body .workbench .g-dosier {
      padding: 0!important;
    }
    body .workbench .g-dosier table {
      page-break-inside: auto;
    }
    body .workbench .g-dosier tr {
      page-break-inside: avoid;
      page-break-after: auto;
    }
    body .workbench .g-dosier article {
      padding: 0px!important;
    }
    body .workbench .g-dosier section {
      padding: 0px!important;
      display: block;
    }
    body .workbench .g-dosier .g-dosier--sidebar {
      display: none;
    }
    body .workbench .g-dosier .articles,
    body .workbench .g-dosier .sections {
      display: block!important;
      padding: 0!important;
    }
    body .workbench .g-dosier .g-dosier-wrapper {
      height: auto;
      width: auto!important;
    }

</style>

<script>
    @Html.Raw(ANBR.Helpful.Misc.Js.Helper.JSVariable("wreportData", Model))
</script>

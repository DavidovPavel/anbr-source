@{
    Layout = null;
}

<!DOCTYPE html>

<html>
    <head>
        <title></title>
        <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
        <style>
            #inLoad div { float: left;margin: 2px;border: solid 1px #ccc;padding: 2px;}
        </style>
    </head>
    <body>
        <div style="width: 80%" id="inLoad"></div>
        <script type="text/javascript">
            $(function() {
                var data = [
                    { id: 31, param: "" },
                    { id: 31, param: "" },
                    { id: 31, param: "" },
                    { id: 31, param: "" },
                    { id: 31, param: "" },
                    { id: 31, param: "" }
                ];

                function mas() {
                    var w = $("#inLoad").width() / 3 - 8 * 3;
                    $("#inLoad div").css({ width: w, height: w });
                    $("#inLoad div iframe").css("height", w - 2)
                }

                $(window).resize(function () {mas();});

                var _tmpl = "<div><iframe style='border:0;width:99%'></iframe></div>";
                var w = $("#inLoad").width() / 3 - 8 * 3;
                for(var i=0;i<data.length;i++) {
                    var el = data[i];
                    var src = "result/" + el.id + "/?param=" + el.param;
                    var $el = $(_tmpl);
                    $el.css({ width: w, height: w });
                    $el.find("iframe").attr("src", src).css("height", w-2);
                    $("#inLoad").append($el);
                }
                
            });
        </script>
    </body>
</html>